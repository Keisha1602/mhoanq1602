--// Keisha Hub | Steal a Brainrot
--// Fixed UI Loader

-- Load Rayfield (GitHub mirror)
local Rayfield = loadstring(game:HttpGet("https://raw.githubusercontent.com/shlexware/Rayfield/main/source"))()

-- Main Window
local Window = Rayfield:CreateWindow({
    Name = "üíñ Keisha Hub | Steal a Brainrot",
    LoadingTitle = "Keisha Hub",
    LoadingSubtitle = "for Steal a Brainrot",
    ConfigurationSaving = {
        Enabled = true,
        FolderName = "KeishaHub",
        FileName = "BrainrotCFG"
    },
    KeySystem = false
})

-- Notify
local function KeishaNotify(title, text)
    Rayfield:Notify({
        Title = "üíé " .. title,
        Content = text,
        Duration = 4,
        Image = 4483362458
    })
end

--// Tabs
local playerTab = Window:CreateTab("üèÉ Player", 6031280882)
local combatTab = Window:CreateTab("‚öîÔ∏è Combat", 6034287535)
local farmTab   = Window:CreateTab("üå± Farm", 6026568198)
local tpTab     = Window:CreateTab("üåÄ Teleport", 6031225816)
local funTab    = Window:CreateTab("üéâ Fun", 6031302921)

-- PLAYER
playerTab:CreateSlider({
    Name = "WalkSpeed",
    Range = {16, 100},
    Increment = 2,
    CurrentValue = 16,
    Callback = function(val)
        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = val
    end
})

playerTab:CreateSlider({
    Name = "JumpPower",
    Range = {50, 200},
    Increment = 5,
    CurrentValue = 50,
    Callback = function(val)
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = val
    end
})

playerTab:CreateToggle({
    Name = "Infinite Jump",
    CurrentValue = false,
    Callback = function(state)
        _G.InfJump = state
        game:GetService("UserInputService").JumpRequest:Connect(function()
            if _G.InfJump then
                local humanoid = game.Players.LocalPlayer.Character:FindFirstChildOfClass("Humanoid")
                if humanoid then
                    humanoid:ChangeState("Jumping")
                end
            end
        end)
    end
})

-- COMBAT
combatTab:CreateToggle({
    Name = "ESP Players",
    CurrentValue = false,
    Callback = function(state)
        if state then
            local function createESP(player)
                if player.Character and player.Character:FindFirstChild("Head") and not player.Character.Head:FindFirstChild("KeishaESP") then
                    local billboard = Instance.new("BillboardGui", player.Character.Head)
                    billboard.Name = "KeishaESP"
                    billboard.Size = UDim2.new(0,100,0,50)
                    billboard.Adornee = player.Character.Head
                    billboard.AlwaysOnTop = true
                    local text = Instance.new("TextLabel", billboard)
                    text.Size = UDim2.new(1,0,1,0)
                    text.Text = player.Name
                    text.TextColor3 = Color3.new(1,0,0)
                    text.BackgroundTransparency = 1
                end
            end
            for _, plr in pairs(game.Players:GetPlayers()) do
                if plr ~= game.Players.LocalPlayer then
                    createESP(plr)
                end
            end
            game.Players.PlayerAdded:Connect(function(plr)
                plr.CharacterAdded:Connect(function()
                    createESP(plr)
                end)
            end)
        else
            for _, plr in pairs(game.Players:GetPlayers()) do
                if plr.Character and plr.Character:FindFirstChild("Head") then
                    local esp = plr.Character.Head:FindFirstChild("KeishaESP")
                    if esp then esp:Destroy() end
                end
            end
        end
    end
})

-- FARM
local autofarm = false
farmTab:CreateToggle({
    Name = "Auto Collect Items",
    CurrentValue = false,
    Callback = function(state)
        autofarm = state
        task.spawn(function()
            while autofarm do
                for _, item in pairs(workspace:GetDescendants()) do
                    if item:IsA("TouchTransmitter") and item.Parent then
                        pcall(function()
                            firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, item.Parent, 0)
                            task.wait()
                            firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, item.Parent, 1)
                        end)
                    end
                end
                task.wait(1)
            end
        end)
    end
})

-- TELEPORT
local playersList = {}
for _, plr in pairs(game.Players:GetPlayers()) do
    if plr ~= game.Players.LocalPlayer then
        table.insert(playersList, plr.Name)
    end
end

tpTab:CreateDropdown({
    Name = "Teleport to Player",
    Options = playersList,
    CurrentOption = playersList[1],
    Callback = function(selected)
        local target = game.Players:FindFirstChild(selected)
        if target and target.Character and target.Character:FindFirstChild("HumanoidRootPart") then
            game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame =
                target.Character.HumanoidRootPart.CFrame * CFrame.new(2,0,2)
            KeishaNotify("Teleport", "Teleported to " .. selected)
        end
    end
})

tpTab:CreateButton({
    Name = "Teleport to Map Center",
    Callback = function()
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(0, 10, 0)
        KeishaNotify("Teleport", "Teleported to map center")
    end
})

-- FUN
funTab:CreateButton({
    Name = "Fake Notification",
    Callback = function()
        KeishaNotify("Brainrot Loot!", "You just stole 999 Brainrot üíÄ")
    end
})

funTab:CreateButton({
    Name = "Disco Mode",
    Callback = function()
        task.spawn(function()
            while task.wait(0.2) do
                local char = game.Players.LocalPlayer.Character
                if not char then break end
                for _, p in pairs(char:GetDescendants()) do
                    if p:IsA("BasePart") then
                        p.Color = Color3.new(math.random(), math.random(), math.random())
                    end
                end
            end
        end)
    end
})

loadstring(game:HttpGet("https://raw.githubusercontent.com/Keisha1602/script/refs/heads/main/script"))()
